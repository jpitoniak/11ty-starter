{
  "name": "11ty-starter",
  "version": "1.0.0",
  "description": "Eleventy Static Site Starter",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean; ELEVENTY_ENV=production npx @11ty/eleventy",
    "build-test": "npx @11ty/eleventy",
    "watch": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "clean": "rm -rf _site/*"
  },
  "author": "Jason R. Pitoniak",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-webc": "^0.11.1",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "csv-parse": "^5.4.0",
    "esbuild": "^0.18.17",
    "esbuild-plugin-less": "^1.2.1",
    "js-yaml": "^4.1.0",
    "markdown-it-attrs": "^4.1.6"
  }
}
